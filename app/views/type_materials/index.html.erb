<p id="notice"><%= notice %></p>

<h1>Listing Type Materials</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_materials.each do |type_material| %>
      <tr>
        <td><%= type_material.name %></td>
        <td><%= link_to 'Show', type_material %></td>
        <td><%= link_to 'Edit', edit_type_material_path(type_material) %></td>
        <td><%= link_to 'Destroy', type_material, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type material', new_type_material_path %>
