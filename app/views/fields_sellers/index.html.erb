<p id="notice"><%= notice %></p>

<h1>Listing Fields Sellers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fields_sellers.each do |fields_seller| %>
      <tr>
        <td><%= fields_seller.name %></td>
        <td><%= link_to 'Show', fields_seller %></td>
        <td><%= link_to 'Edit', edit_fields_seller_path(fields_seller) %></td>
        <td><%= link_to 'Destroy', fields_seller, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fields seller', new_fields_seller_path %>
