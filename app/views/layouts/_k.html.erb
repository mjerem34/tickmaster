var k = [38, 38, 40, 40, 37, 39, 37, 39, 66, 65],
n = 0;
$(document).keydown(function (e) {
  if (e.keyCode === k[n++]) {
      if (n === k.length) {
          // alert('Konami !!!'); // Ã  remplacer par votre code
          window.location.replace("https://archive.org/details/arcade_outrun");
          n = 0;
          return false;
      }
  }
  else {
      n = 0;
  }
});
</script>
