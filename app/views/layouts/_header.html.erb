<header class="main-header">

  <!-- Logo -->
  <%= link_to root_path, class: 'logo' do %>
  <!-- mini logo for sidebar mini 50x50 pixels -->
    <span class="logo-mini"><b>t</b>m</span>
    <!-- logo for regular state and mobile devices -->
    <span class="logo-lg"><b>tick'</b>master</span>
  <% end %>

  <!-- Header Navbar -->
  <nav class="navbar navbar-static-top" role="navigation">
    <!-- Sidebar toggle button-->
    <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
      <span class="sr-only">Toggle navigation</span>
    </a>
    <!-- Navbar Right Menu -->
    <div class="navbar-custom-menu">
      <ul class="nav navbar-nav">
        <!-- Messages: style can be found in dropdown.less-->
        <li class="dropdown messages-menu">
          <!-- Menu toggle button -->
          <a href="#" class="dropdown-toggle" data-toggle="dropdown">
            <i class="fa fa-envelope-o"></i>
            <span class="label label-success">4</span>
          </a>
          <ul class="dropdown-menu">
            <li class="header">You have 4 messages</li>
            <li>
              <!-- inner menu: contains the messages -->
              <ul class="menu">
                <li><!-- start message -->
                  <a href="#">
                    <div class="pull-left">
                      <!-- User Image -->
                      <img src="dist/img/user2-160x160.jpg" class="img-circle" alt="User Image">
                    </div>
                    <!-- Message title and timestamp -->
                    <h4>
                      Support Team
                      <small><i class="fa fa-clock-o"></i> 5 mins</small>
                    </h4>
                    <!-- The message -->
                    <p>Why not buy a new awesome theme?</p>
                  </a>
                </li>
                <!-- end message -->
              </ul>
              <!-- /.menu -->
            </li>
            <li class="footer"><a href="#">See All Messages</a></li>
          </ul>
        </li>
        <!-- /.messages-menu -->

        <!-- Notifications Menu -->
        <li class="dropdown notifications-menu">
          <!-- Menu toggle button -->
          <a href="#" class="dropdown-toggle" data-toggle="dropdown">
            <i class="fa fa-bell-o"></i>
            <span class="label label-warning">10</span>
          </a>
          <ul class="dropdown-menu">
            <li class="header">You have 10 notifications</li>
            <li>
              <!-- Inner Menu: contains the notifications -->
              <ul class="menu">
                <li><!-- start notification -->
                  <a href="#">
                    <i class="fa fa-users text-aqua"></i> 5 new members joined today
                  </a>
                </li>
                <!-- end notification -->
              </ul>
            </li>
            <li class="footer"><a href="#">View all</a></li>
          </ul>
        </li>
        <%= "
        <!-- Tasks Menu -->

        <li class='dropdown tasks-menu'>
          <!-- Menu Toggle Button -->
          <a href='#' class='dropdown-toggle' data-toggle='dropdown'>
            <i class='fa fa-cloud-upload'></i>
          </a>
          <ul class='dropdown-menu'>
            <li class='header'>Historique des mises à jour</li>
            <li>
              <ul class='menu'>
                <li>#{link_to(Update.last.changesMade.html_safe, update_url(Update.last))}</li>
              </ul>
            </li>
            <li class='footer'>
            #{link_to("Nouvelle mise à jour", new_update_url) if @create_update }
            </li>
          </ul>
        </li>
        ".html_safe if @view_update %>
        <!-- User Account Menu -->
        <li class="dropdown user user-menu">
          <!-- Menu Toggle Button -->
          <a href="#" class="dropdown-toggle" data-toggle="dropdown">
            <!-- The user image in the navbar-->
            <!-- <img src="dist/img/user2-160x160.jpg" class="user-image" alt="User Image"> -->
            <!-- hidden-xs hides the username on small devices so only the image appears. -->
            <span class="hidden-xs"><%= "#{current_user.surname} #{current_user.name}" %></span>
          </a>
          <ul class="dropdown-menu">
            <!-- The user image in the menu -->
            <li class="user-header">
              <!-- <img src="dist/img/user2-160x160.jpg" class="img-circle" alt="User Image"> -->
              <p>
                <%= "#{current_user.surname} #{current_user.name}" %> - <%= "#{current_user.type_user.name}"%>
                <small>Membre depuis <%= "#{current_user.created_at.strftime("%b. %Y")}" %></small>
              </p>
            </li>
            <!-- Menu Body -->
            <!-- <li class="user-body">
              <div class="row">
                <div class="col-xs-4 text-center">
                  <a href="#">Followers</a>
                </div>
                <div class="col-xs-4 text-center">
                  <a href="#">Sales</a>
                </div>
                <div class="col-xs-4 text-center">
                  <a href="#">Friends</a>
                </div>
              </div>
            </li> -->
            <!-- Menu Footer-->
            <li class="user-footer">
              <div class="pull-left">
                <%= link_to "Profil", edit_user_url(current_user), class: "btn btn-flat btn-default btn-flat" %>
              </div>
              <div class="pull-right">
                <%= link_to "Déconnexion", signout_url, class: "btn btn-flat btn-default btn-flat", method: :delete %>
              </div>
            </li>
          </ul>
        </li>
        <!-- Control Sidebar Toggle Button -->
        <li>
          <%= link_to "", new_incident_url, class: "fa fa-ticket" %>
          <!-- <a href="#" data-toggle="control-sidebar"><i class="fa fa-gears"></i></a> -->
        </li>
      </ul>
    </div>
  </nav>
</header>
<!-- Left side column. contains the logo and sidebar -->
<aside class="main-sidebar">
  <div class="slimScrollDiv" style="position: relative; overflow: hidden; width: auto; height: 534px;">

    <!-- sidebar: style can be found in sidebar.less -->
    <section class="sidebar" style="height: 534px; overflow: hidden; width: auto;">

      <!-- Sidebar user panel (optional) -->
      <div class="user-panel">
        <div class="pull-left image">
          <img src="dist/img/user2-160x160.jpg" class="img-circle" alt="User Image" style="color:transparent;">
        </div>
        <div class="pull-left info">
          <p>Bonjour <%= current_user.name %></p>
          <!-- Status -->
          <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
        </div>
      </div>

      <!-- search form (Optional) -->
      <form action="#" method="get" class="sidebar-form">
        <div class="input-group">
          <input type="text" name="q" class="form-control" placeholder="Rechercher...">
          <span class="input-group-btn">
            <button type="submit" name="search" id="search-btn" class="btn btn-flat btn-flat"><i class="fa fa-search"></i>
            </button>
          </span>
        </div>
      </form>
      <!-- /.search form -->

      <!-- Sidebar Menu -->
      <ul class="sidebar-menu">
        <!-- <li class="header">HEADER</li> -->
        <!-- Optionally, you can add icons to the links -->
        <!-- <li class="active"><a href="#"><i class="fa fa-link"></i> <span>Procedures</span></a></li> -->
        <!-- <li><a href="#"><i class="fa fa-link"></i> <span>Another Link</span></a></li> -->
        <!-- Procedures links -->
        <li class="treeview">
          <a href="#"><i class="fa fa-bookmark"></i> <span>Procedures</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <%= "<li>".html_safe + link_to('Liste des procedures', procedures_url) + "</li>".html_safe if @view_procedures %>
            <%= "<li>".html_safe + link_to('Créer procedures', new_procedure_url) + "</li>".html_safe if @create_procedure %>
          </ul>
        </li>
        <li class="treeview">
          <a href="#"><i class="fa fa-ticket"></i> <span>Incidents</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <%= "<li>".html_safe + link_to("Mes incidents", current_user) + "</li>".html_safe %>
            <%= "<li>".html_safe + link_to("A traiter", to_treat_user_url(current_user)) + "</li>".html_safe if @without_tech_incidents %>
            <%= "<li>".html_safe + link_to("Non attribués", without_tech_incidents_url) + "</li>".html_safe if @without_tech_incidents %>
            <%= "<li>".html_safe + link_to("Tous les incidents", incidents_url) + "</li>".html_safe if @index_incidents %>
            <%= "<li>".html_safe + link_to("Cloturés/Rejetés", allincidents_user_url(current_user)) + "</li>".html_safe %>
          </ul>
        </li>
        <li class="treeview">
          <a href="#"><i class="fa fa-sitemap"></i> <span>Catégories</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <%= "<li>".html_safe + link_to("Catégories", categories_url) + "</li>".html_safe if @index_categories %>
            <%= "<li>".html_safe + link_to("Sous-Catégories", sous_categories_url) + "</li>".html_safe if @index_categories %>
          </ul>
        </li>
        <li class="treeview">
          <a href="#"><i class="fa fa-users"></i> <span>Utilisateurs</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <%= "<li>".html_safe + link_to("Tous les utilisateurs", users_url) + "</li>".html_safe if @index_users %>
            <%= "<li>".html_safe + link_to("Créer un utilisateur", new_type_user_url) + "</li>".html_safe if @create_new_tech %>
            <%= "<li>".html_safe + link_to("Champs utilisateurs", field_type_users_url) + "</li>".html_safe if @index_field_type_users %>
          </ul>
        </li>
        <li class="treeview">
          <a href="#"><i class="fa fa-sliders"></i> <span>Droits</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <%= "<li>".html_safe + link_to("Liste des droits", rights_url) + "</li>".html_safe if @view_index_rights %>
            <%= "<li>".html_safe + link_to("Profils utilisateurs", type_users_url) + "</li>".html_safe if @view_type_users %>
          </ul>
        </li>
        <li class="treeview">
          <a href="#"><i class="fa fa-building"></i> <span>Agences</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <%= "<li>".html_safe + link_to("Liste des agences", agencies_url) + "</li>".html_safe if @index_agencies %>
            <%= "<li>".html_safe + link_to("Nouvelle agence", new_agency_url) + "</li>".html_safe if @new_agencies %>
            <%= "<li>".html_safe + link_to("Champs agences", field_agencies_url) + "</li>".html_safe if @index_field_agencies %>
            <%= "<li>".html_safe + link_to("Pinger les agences", ping_agencies_url) + "</li>".html_safe if @ping_agencies %>
          </ul>
        </li>
        <li class="treeview">
          <a href="#"><i class="fa fa-wrench"></i> <span>Matériel</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <%= "<li>".html_safe + link_to("Nouveau matériel", new_material_url) + "</li>".html_safe if @create_material %>
            <%= "<li>".html_safe + link_to("Liste des matériels", materials_url) + "</li>".html_safe if @view_material %>
            <%= "<li>".html_safe + link_to("Liste des vendeurs", sellers_url) + "</li>".html_safe if @view_sellers %>
            <%= "<li>".html_safe + link_to("Champs vendeurs", field_sellers_url) + "</li>".html_safe if @index_field_sellers %>
            <%= "<li>".html_safe + link_to("Types de matériels", type_materials_url) + "</li>".html_safe if @view_type_material %>
            <%= "<li>".html_safe + link_to("Types de caractéristiques techniques", spec_type_materials_url) + "</li>".html_safe if @view_spec_type_material %>
            <%= "<li>".html_safe + link_to("Caractéristiques techniques", spec_materials_url) + "</li>".html_safe if @view_spec_material %>
          </ul>
        </li>
        <li class="treeview">
          <a href="#"><i class="fa fa-question"></i> <span>Aide</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <%= "<li>".html_safe + link_to("FAQ", pages_help_url) + "</li>".html_safe %>
            <%= "<li>".html_safe + mail_to("jeremy.montesinos@cle-expertises.fr", "Bug/Suggestion", cc: "jerome.guillot@cle-expertises.fr", subject: "Bug ou suggestion à propos de tick'master") + "</li>".html_safe %>
          </ul>
        </li>

      </ul>
      <!-- /.sidebar-menu -->
    </section>
    <div class="slimScrollBar" style="background: rgba(0, 0, 0, 0.2) none repeat scroll 0% 0%; width: 3px; position: absolute; top: 210px; opacity: 0.4; display: none; border-radius: 7px; z-index: 99; right: 1px; height: 324.41px;"></div>
    <div class="slimScrollRail" style="width: 3px; height: 100%; position: absolute; top: 0px; display: none; border-radius: 7px; background: rgb(51, 51, 51) none repeat scroll 0% 0%; opacity: 0.2; z-index: 90; right: 1px;"></div>
  </div>
    <!-- /.sidebar -->
</aside>
