<%= simple_form_for(@procedure) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nom %>
    <%= f.input :contenu %>
    <%= f.input :resolution %>
    <%= f.input :category_id %>
    <%= f.input :sous_category_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
