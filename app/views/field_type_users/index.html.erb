<p id="notice"><%= notice %></p>

<h1>Listing Field Type Users</h1>

<table>
  <thead>
    <tr>
      <th>Type user</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @field_type_users.each do |field_type_user| %>
      <tr>
        <td><%= field_type_user.type_user_id %></td>
        <td><%= field_type_user.name %></td>
        <td><%= link_to 'Show', field_type_user %></td>
        <td><%= link_to 'Edit', edit_field_type_user_path(field_type_user) %></td>
        <td><%= link_to 'Destroy', field_type_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

