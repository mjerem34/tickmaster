<%= simple_form_for(@material) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :type_material_id %>
    <%= f.input :detentor_type_id %>
    <%= f.input :detentor_id %>
    <%= f.input :seller_id %>
    <%= f.input :date %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
